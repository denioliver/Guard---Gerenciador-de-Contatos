{
  "name": "guard-gerenciador-de-contatos-root",
  "private": true,
  "workspaces": [
    "FrontEnd",
    "BackEnd"
  ],
  "scripts": {
    "install-all": "npm --prefix FrontEnd install && npm --prefix BackEnd install",
    "front": "npm --prefix FrontEnd run dev -- --open",
    "back": "npm --prefix BackEnd start",
    "dev": "npm-run-all --parallel back front"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}